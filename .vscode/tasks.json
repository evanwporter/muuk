{
  "tasks": [
    {
      "type": "shell",
      "label": "Build Muuk Debug",
      "command": "muuk",
      "args": [
        "build"
        // "--release"
      ],
      "problemMatcher": ["$msCompile"],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "type": "cppbuild",
      "label": "C/C++: cl.exe build active file",
      "command": "cl.exe",
      "args": [
        "/Zi",
        "/EHsc",
        "/nologo",
        "/std:c++20",
        "/Fe${fileDirname}\\${fileBasenameNoExtension}.exe",
        "${file}"
      ],
      "options": {
        "cwd": "${fileDirname}"
      },
      "problemMatcher": ["$msCompile"],
      "detail": "Task generated by Debugger."
    }
  ],
  "version": "2.0.0"
}
