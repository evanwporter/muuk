  "version": "0.2.0",
  "configurations": [
    {
      "name": "(Windows) Build",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/debug/muuk.exe",
      "args": [
        "build",
        "-c",
        "cl",
        "--profile",
        "release",
        // "install"
        // "add",
        // "Neargye/magic_enum"
        // "aappleby/smhasher"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "console": "integratedTerminal",
      "symbolSearchPath": "${workspaceFolder}/build/debug",
      "preLaunchTask": "Build Debug (MSVC)"
    },
    {
      "name": "(Windows) Build Clang",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/debug/muuk.exe",
      "args": [
        "build",
        "-c",
        "clang",
        "--profile",
        "release"
        // "install"
        // "add",
        // "Neargye/magic_enum"
        // "aappleby/smhasher"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "console": "integratedTerminal",
      "symbolSearchPath": "${workspaceFolder}/build/debug",
      "preLaunchTask": "Build Debug (MSVC)"
    },
    {
      "name": "(gdb) Launch",
      "type": "cppdbg",
      "request": "launch",
      "program": "builddir/release/bin/muuk",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${fileDirname}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
        {
          "description": "Set Disassembly Flavor to Intel",
          "text": "-gdb-set disassembly-flavor intel",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "Build Debug (GCC)"
    },
    {
      "name": "(Windows) Install",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/debug/muuk.exe",
      "args": [
        "install"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "console": "integratedTerminal",
      "symbolSearchPath": "${workspaceFolder}/build/debug",
      // "preLaunchTask": "Build Debug (MSVC)"
    },
    {
      "name": "(Windows) Add Package",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/debug/muuk.exe",
      "args": [
        "add",
        "Neargye/magic_enum"
        // "aappleby/smhasher"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "console": "integratedTerminal",
      "symbolSearchPath": "${workspaceFolder}/build/debug",
      "preLaunchTask": "Build Debug (MSVC)"
    },
    {
      "name": "(Windows) Remove Package",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/debug/muuk.exe",
      "args": [
        "remove",
        "magic_enum"
        // "aappleby/smhasher"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "console": "integratedTerminal",
      "symbolSearchPath": "${workspaceFolder}/build/debug",
    },
    {
      "name": "(Windows) Run Script",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/debug/muuk.exe",
      "args": [
        "run",
        "muuk"
        // "aappleby/smhasher"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "console": "integratedTerminal",
      "symbolSearchPath": "${workspaceFolder}/build/debug",
      "preLaunchTask": "Build Debug (MSVC)"
    },
  ]
}